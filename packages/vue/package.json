{
  "name": "@w2ui/icons",
  "version": "1.0.0",
  "description": "w2 ui icons set.",
  "type": "module",
  "keywords": ["icons", "w2-ui", "practice"],
  "author": "lnnlmario",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lnnlmario/w2-ui-icons.git",
    "directory": "packages/vue"
  },
  "files": ["dist"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./dist/types/*"
      ]
    }
  },
  "scripts": {
    "build": "pnpm run build:generate && run-p build:build build:types",
    "build:build": "tsx build/build.ts",
    "build:generate": "tsx build/generate.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@element-plus/icons-svg": "^2.1.0",
    "@types/fs-extra": "^11.0.1",
    "camelcase": "^7.0.1",
    "chalk": "^5.2.0",
    "consola": "^3.0.2",
    "esbuild": "^0.17.17",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "tsx": "^3.12.6",
    "unplugin-vue": "^4.1.0",
    "vite": "^4.2.2",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  }
}
